<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>ankitmehtame.restclientapp</id>
    <version>1.0</version>
    <authors>Ankit Mehta</authors>
    <projectUrl>https://github.com/ankitmehtame/rest-client-app</projectUrl>
    <tags>ankitmehtame rest client</tags>
    <summary>Make POST REST call via command line</summary>
    <description>Make POST REST call via command line (with no content) Can provide URL via file and content body via file, with 1st and 2nd arguments respectively</description>
  </metadata>
  <dependencies>
    <group targetFramework="netcoreapp3.1" />
  </dependencies>
  <files>
    <file src="bin\Release\**\*.*" target="lib" exclude="**/*.dev.json;**/*.ps1;**/*.txt" />
    <!-- <file src="bin/Release/**/*.*" target="tools" /> -->
    <file src="bin\Release\netcoreapp*\tools\*.*" target="tools" />
  </files>
</package>
